{
  "name": "bookingtool-server-node",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "PORT=3003 NODE_ENV=production forever start dist/server/bin/www.js",
    "develop": "concurrently --kill-others \"ng serve --progress=false --proxy-config proxy.conf.json\" \"npm run _server:run\"",
    "build": "ng build --prod --base-href=/pt2/ --sm=false --aot=true --output-path=dist/client && npm run _server:build",
    "test": "ng test --watch=false",
    "lint": "ng lint \"src/**/*.ts\"",
    "e2e": "ng e2e",
    "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",
    "_server:build": "tsc -p ./server",
    "postinstall": "npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.1",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "angular-2-local-storage": "^1.0.1",
    "angular2-jwt": "^0.2.0",
    "chart.js": "^2.5.0",
    "concurrently": "^3.4.0",
    "core-js": "^2.4.1",
    "express-jwt": "^5.1.0",
    "jsonwebtoken": "^7.3.0",
    "mysql": "^2.13.0",
    "primeng": "^4.0.0-rc.2",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.11.0",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  }
}
